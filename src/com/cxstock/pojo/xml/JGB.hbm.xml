<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.cxstock.pojo.Jgb" table="Jgb">
    <id name="jgid" type="java.lang.String" >
    	<column name="jgid" length="20"/>
    </id>
    <property name="jgmc" type="java.lang.String">
        <column name="jgmc" length="100"/>
    </property>
    <property name="pid" type="java.lang.String">
        <column name="pid" length="20"/>
    </property>
 </class>
</hibernate-mapping>